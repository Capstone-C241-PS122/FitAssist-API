generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model article {
  id          Int    @id
  Title       String
  Description String
  Type        String
  BodyPart    String
  Equipment   String
  Level       String
  library        library[]
}

model feedback {
  id          Int    @id @default(autoincrement())
  description String
}

model library {
  id_library  Int     @id @default(autoincrement())
  vidId       Int?
  articleId   Int?
  type        String
  title       String?
  description String?
  video       video?   @relation(fields: [vidId], references: [id])
  article     article? @relation(fields: [articleId], references: [id])
  videoT       video?   @relation(fields: [title], references: [name_exercise])
  articleT     article? @relation(fields: [title], references: [Title])
}

model nutrition {
  id           Int    @id
  calories     Int
  proteins     Float
  fat          Float
  carbohydrate Float
  name         String
}

model video {
  id             Int       @id
  name_exercise  String
  description    String
  name_equipment String
  url_video      String
  label          String
  library        library[]
}
