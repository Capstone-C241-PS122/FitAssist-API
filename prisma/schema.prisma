generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model article {
  id            Int    @id
  Title         String
  Description   String
  Type          String
  BodyPart      String
  Equipment     String
  Level         String
}

model video {
  id             Int       @id
  name_exercise  String
  description    String
  name_equipment String
  bodypart       String
  url_video      String
  label          String
  libraries      library[]
}

model library { 
  id              Int     @id @default(autoincrement())
  vidId           Int
  vidio           video   @relation(fields: [vidId], references: [id])
  name_exercise   String?  
  url_video       String?  
  bodypart        String?  
  name_equipment  String?  
}


model nutrition {
  id           Int    @id
  calories     Int
  proteins     Float
  fat          Float
  carbohydrate Float
  name         String
}